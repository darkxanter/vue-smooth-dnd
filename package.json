{
  "name": "vue-smooth-dnd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build && vue-tsc -p ./tsconfig.build.json --emitDeclarationOnly",
    "type-check": "vue-tsc -p ./tsconfig.build.json --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/vue-smooth-dnd.umd.js",
  "module": "./dist/vue-smooth-dnd.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-smooth-dnd.es.js",
      "require": "./dist/vue-smooth-dnd.umd.js"
    }
  },
  "dependencies": {
    "smooth-dnd": "^0.12.1"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.12"
  }
}
